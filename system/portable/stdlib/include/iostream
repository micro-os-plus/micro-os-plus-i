#ifndef IOSTREAM_
#define IOSTREAM_

#include "portable/stdlib/include/istream"
#include "portable/stdlib/include/ostream"

class iostream : public ostream, public istream
  {
public:
  iostream(streambuf* sb);
  virtual ~iostream();
  };

#endif /*IOSTREAM_*/
